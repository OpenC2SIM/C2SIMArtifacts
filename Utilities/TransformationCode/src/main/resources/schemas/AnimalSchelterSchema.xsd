    </xs:annotation>
    <xs:sequence>
      <xs:element ref="IsoTimeDuration" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Duration" type="DurationType" />
  <xs:complexType name="TaskStatusType">
