    </xs:annotation>
    <xs:sequence>
      <xs:element ref="Task" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="TaskRequestContent" type="TaskRequestContentType" />
