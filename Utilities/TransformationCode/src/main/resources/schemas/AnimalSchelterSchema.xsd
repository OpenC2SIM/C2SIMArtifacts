      <xs:element ref="DateTime" minOccurs="0" maxOccurs="1" />
      <xs:element ref="FromSendingSystem" minOccurs="1" maxOccurs="1" />
      <xs:element ref="InReplyToMessageID" minOccurs="0" maxOccurs="1" />
      <xs:element ref="MessageID" minOccurs="1" maxOccurs="1" />
      <xs:element ref="ReplyToSystem" minOccurs="0" maxOccurs="1" />
      <xs:element ref="SecurityClassificationCode" minOccurs="0" maxOccurs="1" />
      <xs:element ref="ToReceivingSystem" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="C2SIMHeader" type="C2SIMHeaderType" />
  <xs:complexType name="EventType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#Event</xs:documentation>
      <xs:documentation>"An ACTION that is an incident, phenomenon, or occasion. May or may not be the result of an agent's action."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="ActionTemporalRelationship" minOccurs="0" maxOccurs="unbounded" />
      <xs:element ref="Duration" minOccurs="0" maxOccurs="1" />
      <xs:element ref="EventCode" minOccurs="1" maxOccurs="1" />
      <xs:element ref="Location" minOccurs="0" maxOccurs="unbounded" />
      <xs:element ref="MapGraphicID" minOccurs="0" maxOccurs="unbounded" />
      <xs:element ref="Name" minOccurs="0" maxOccurs="1" />
      <xs:element ref="TimeInstant" minOccurs="1" maxOccurs="1" />
      <xs:element ref="UUID" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Event" type="EventType" />
