  <xs:complexType name="MessageCodeType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#MessageCode</xs:documentation>
      <xs:documentation>"Enumerations used in messages."</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element ref="SystemCommandTypeCode" />
      <xs:element ref="MIPRequestCategoryCode" />
      <xs:element ref="CommunicativeActTypeCode" />
      <xs:element ref="TaskStatusCode" />
      <xs:element ref="AcknowledgeTypeCode" />
    </xs:choice>
  </xs:complexType>
  <xs:element name="MessageCode" type="MessageCodeType" />
  <xs:complexType name="ReportContentType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ReportContent</xs:documentation>
      <xs:documentation>"This is a collection class that may be used for different types of messages reporting information to other systems."</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element ref="PositionReportContent" />
      <xs:element ref="ObservationReportContent" />
      <xs:element ref="TaskStatus" />
    </xs:choice>
  </xs:complexType>
  <xs:element name="ReportContent" type="ReportContentType" />
  <xs:complexType name="MessageType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#Message</xs:documentation>
      <xs:documentation>"Any C2SIM message that is sent between host systems."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MessageBody" minOccurs="1" maxOccurs="1" />
      <xs:element name="C2SIMHeader" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Message" type="MessageType" />
  <xs:complexType name="C2SIMHeaderType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#C2SIMHeader</xs:documentation>
      <xs:documentation>"Indicates that the receiving system should submit its initialization data to the system identified in the"</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="FromSendingSystem" minOccurs="1" maxOccurs="1" />
      <xs:element name="InReplyToMessageID" minOccurs="0" maxOccurs="1" />
      <xs:element name="ConversationID" minOccurs="1" maxOccurs="1" />
      <xs:element name="ToReceivingSystem" minOccurs="1" maxOccurs="1" />
      <xs:element name="MessageID" minOccurs="1" maxOccurs="1" />
      <xs:element name="ReplyToSystem" minOccurs="0" maxOccurs="1" />
      <xs:element name="SendingTime" minOccurs="0" maxOccurs="1" />
      <xs:element name="SecurityClassificationCode" minOccurs="0" maxOccurs="1" />
      <xs:element name="CommunicativeActTypeCode" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="C2SIMHeader" type="C2SIMHeaderType" />
  <xs:complexType name="MessageBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#MessageBody</xs:documentation>
      <xs:documentation>"The structure for sending domain messages: orders, reports, and acknowledgements."</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element ref="SystemAcknowledgementBody" />
      <xs:element ref="DomainMessageBody" />
      <xs:element ref="ObjectInitializationBody" />
      <xs:element ref="SystemCommandBody" />
      <xs:element ref="C2SIMInitializationBody" />
    </xs:choice>
  </xs:complexType>
  <xs:element name="MessageBody" type="MessageBodyType" />
  <xs:complexType name="MIPRequestContentType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/smx#MIPRequestContent</xs:documentation>
      <xs:documentation>"A request whose category is defined by the JC3IEDM request category code."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MIPRequestCategoryCode" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="MIPRequestContent" type="MIPRequestContentType" />
  <xs:complexType name="TaskRequestContentType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#TaskRequestContent</xs:documentation>
      <xs:documentation>"A request for an Actor to perform a Task. The Actor to perform the Task is identified by the hasPerformingEntity property of the Task."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Task" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="TaskRequestContent" type="TaskRequestContentType" />
  <xs:simpleType name="SystemCommandTypeCodeType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#SystemCommandTypeCode</xs:documentation>
      <xs:documentation>"The codes (enumerated individuals) that indicate what command is being sent in a system command message."</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="ShareScenario" />
      <xs:enumeration value="SubmitInitialization" />
      <xs:enumeration value="StopScenario" />
      <xs:enumeration value="StartScenario" />
      <xs:enumeration value="InitializationComplete" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="SystemCommandTypeCode" type="SystemCommandTypeCodeType" />
  <xs:simpleType name="MIPRequestCategoryCodeType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/smx#MIPRequestCategoryCode</xs:documentation>
      <xs:documentation>"A request category from the Joint Consultation Command Control Information Exchange Data Model (JC3IEDM) developed by the Multilateral Interoperability Programme (MIP)."</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="ACTION" />
      <xs:enumeration value="LOCATN" />
      <xs:enumeration value="HOLDNG" />
      <xs:enumeration value="CAPAB" />
      <xs:enumeration value="ASSOC" />
      <xs:enumeration value="HOSTIL" />
      <xs:enumeration value="PRESNC" />
      <xs:enumeration value="TYPE" />
      <xs:enumeration value="STATUS" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="MIPRequestCategoryCode" type="MIPRequestCategoryCodeType" />
  <xs:simpleType name="CommunicativeActTypeCodeType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#CommunicativeActTypeCode</xs:documentation>
      <xs:documentation>"Enumeration of different types of communications."</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Refuse" />
      <xs:enumeration value="Propose" />
      <xs:enumeration value="Inform" />
      <xs:enumeration value="Request" />
      <xs:enumeration value="Confirm" />
      <xs:enumeration value="Accept" />
      <xs:enumeration value="Agree" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="CommunicativeActTypeCode" type="CommunicativeActTypeCodeType" />
  <xs:simpleType name="TaskStatusCodeType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#TaskStatusCode</xs:documentation>
      <xs:documentation>"An enumeration of possible status values for a task."</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="TASKINPRG" />
      <xs:enumeration value="TASKSTRT" />
      <xs:enumeration value="TASKCMPLT" />
      <xs:enumeration value="TASKPEND" />
      <xs:enumeration value="TASKABRT" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="TaskStatusCode" type="TaskStatusCodeType" />
  <xs:simpleType name="AcknowledgeTypeCodeType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AcknowledgeTypeCode</xs:documentation>
      <xs:documentation>"Enumeration of different types of acknowledgements."</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="ACKNOTRECGNZ" />
      <xs:enumeration value="ACKREQDEN" />
      <xs:enumeration value="ACKSUCC" />
      <xs:enumeration value="ACKREQGRT" />
      <xs:enumeration value="ACKRCVD" />
      <xs:enumeration value="ACKNOTEXEC" />
      <xs:enumeration value="ACKFAIL" />
      <xs:enumeration value="ACKNOTUNDSTD" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="AcknowledgeTypeCode" type="AcknowledgeTypeCodeType" />
  <xs:complexType name="PositionReportContentType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#PositionReportContent</xs:documentation>
      <xs:documentation>"The content of a report that is used primarily to convey the position of an entity."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SubjectEntity" minOccurs="0" maxOccurs="1" />
      <xs:element name="Location" minOccurs="0" maxOccurs="1" />
      <xs:element name="EntityHealthStatus" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="TimeOfObservation" minOccurs="1" maxOccurs="1" />
      <xs:element name="Duration" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="PositionReportContent" type="PositionReportContentType" />
  <xs:complexType name="ObservationReportContentType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/smx#ObservationReportContent</xs:documentation>
      <xs:documentation>"This report contains an observation of some aspect of a subject--not necessarily including the location, health, or other facts that are in a position report.
Multiple observations all refer to the same observed actor."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Observation" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="TimeOfObservation" minOccurs="1" maxOccurs="1" />
      <xs:element name="Duration" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ObservationReportContent" type="ObservationReportContentType" />
  <xs:complexType name="TaskStatusType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#TaskStatus</xs:documentation>
      <xs:documentation>"The status of a task, provided before, during or after a task is executed."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CurrentTask" minOccurs="1" maxOccurs="1" />
      <xs:element name="TaskStatusCode" minOccurs="1" maxOccurs="1" />
      <xs:element name="TimeOfObservation" minOccurs="1" maxOccurs="1" />
      <xs:element name="Duration" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="TaskStatus" type="TaskStatusType" />
  <xs:complexType name="SystemAcknowledgementBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#SystemAcknowledgementBody</xs:documentation>
      <xs:documentation>"An acknowledgement from one C2SIM system to another that a message was received."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AcknowledgeTypeCode" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="SystemAcknowledgementBody" type="SystemAcknowledgementBodyType" />
  <xs:complexType name="DomainMessageBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#DomainMessageBody</xs:documentation>
      <xs:documentation>"The body of a message that is sent between actors in the domain.
It is one subclass of MessageBody (other subclasses perform administrative functions in the C2SIM Coalition.)"</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element ref="OrderBody" />
      <xs:element ref="PlanBody" />
      <xs:element ref="AcknowledgementBody" />
      <xs:element ref="ReportBody" />
      <xs:element ref="RequestBody" />
    </xs:choice>
  </xs:complexType>
  <xs:element name="DomainMessageBody" type="DomainMessageBodyType" />
  <xs:complexType name="ObjectInitializationBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ObjectInitializationBody</xs:documentation>
      <xs:documentation>"This message is sent by each system to a marshalling server upon receipt of a SubmitInitialization message."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="InitializationDataFile" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="ScenarioSetting" minOccurs="0" maxOccurs="1" />
      <xs:element name="SystemEntityList" minOccurs="0" maxOccurs="1" />
      <xs:element name="ObjectDefinitions" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ObjectInitializationBody" type="ObjectInitializationBodyType" />
  <xs:complexType name="SystemCommandBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#SystemCommandBody</xs:documentation>
      <xs:documentation>"The body of a message that is used to send system commands between C2SIM systems."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SystemCommandTypeCode" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="SystemCommandBody" type="SystemCommandBodyType" />
  <xs:complexType name="C2SIMInitializationBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#C2SIMInitializationBody</xs:documentation>
      <xs:documentation>"This message can initialize all of the objects in the scenario as well as the scenario setting."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ScenarioSetting" minOccurs="1" maxOccurs="1" />
      <xs:element name="SystemEntityList" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="ObjectDefinitions" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="InitializationDataFile" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="C2SIMInitializationBody" type="C2SIMInitializationBodyType" />
  <xs:complexType name="OrderBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#OrderBody</xs:documentation>
      <xs:documentation>"The structure for sending order messages.
This message may define tasks, or it may refer to tasks defined elsewhere (such as in an initialization message).
An order generally comes from an entity with command authority and thus is expected to be obeyed if it is physically possible to do so."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="RequestingEntity" minOccurs="0" maxOccurs="1" />
      <xs:element name="TaskReference" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="OrderID" minOccurs="1" maxOccurs="1" />
      <xs:element name="ToReceiver" minOccurs="1" maxOccurs="1" />
      <xs:element name="FromSender" minOccurs="1" maxOccurs="1" />
      <xs:element name="IssuedTime" minOccurs="1" maxOccurs="1" />
      <xs:element name="Task" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="Entity" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="OrderBody" type="OrderBodyType" />
  <xs:complexType name="PlanBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/lox#PlanBody</xs:documentation>
      <xs:documentation>"This message orders the recipient unit to begin execution of the indicated plan."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ToBeExecutedNow" minOccurs="1" maxOccurs="1" />
      <xs:element name="PlanPhaseReference" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="ToReceiver" minOccurs="1" maxOccurs="1" />
      <xs:element name="FromSender" minOccurs="1" maxOccurs="1" />
      <xs:element name="PlanPhase" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="PlanBody" type="PlanBodyType" />
  <xs:complexType name="AcknowledgementBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AcknowledgementBody</xs:documentation>
      <xs:documentation>"An acknowledgement from one Actor to another that a message was received."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ToReceiver" minOccurs="1" maxOccurs="1" />
      <xs:element name="FromSender" minOccurs="1" maxOccurs="1" />
      <xs:element name="AcknowledgeTypeCode" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="AcknowledgementBody" type="AcknowledgementBodyType" />
  <xs:complexType name="ReportBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ReportBody</xs:documentation>
      <xs:documentation>"The structure for sending report messages."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ReportID" minOccurs="1" maxOccurs="1" />
      <xs:element name="ReportingEntity" minOccurs="1" maxOccurs="1" />
      <xs:element name="ToReceiver" minOccurs="1" maxOccurs="1" />
      <xs:element name="FromSender" minOccurs="1" maxOccurs="1" />
      <xs:element name="ReportContent" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ReportBody" type="ReportBodyType" />
  <xs:complexType name="RequestBodyType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#RequestBody</xs:documentation>
      <xs:documentation>"The body of a message from an actor in the domain to another actor in the domain that requests something.
A request is fulfilled at the discretion of the entity to which it is directed."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="RequestingEntity" minOccurs="1" maxOccurs="1" />
      <xs:element name="ToReceiver" minOccurs="1" maxOccurs="1" />
      <xs:element name="FromSender" minOccurs="1" maxOccurs="1" />
      <xs:element name="RequestContent" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="RequestBody" type="RequestBodyType" />
  <xs:complexType name="SystemEntityListType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#SystemEntityList</xs:documentation>
      <xs:documentation>"This is a listing of entities that reside within a system. The list defines what system a message should be sent to in order to get it to a given actor.  Each entity can reside on only one system. This list does not define necessarily define the complete list of entities on the given system.
Systems can build a map from this and other lists to get the system from a given entity."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="SystemName" minOccurs="1" maxOccurs="1" />
      <xs:element name="ActorReference" minOccurs="1" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="SystemEntityList" type="SystemEntityListType" />
  <xs:complexType name="InitializationDataFileType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#InitializationDataFile</xs:documentation>
      <xs:documentation>"Defines a file of supplementary data that is used to initialize the scenario. This file is generally system-specific, for example a terrain database file or a mapping from C2SIM entity types to local entity type enumerations. It could apply to \"all\" systems."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IntializationFileType" minOccurs="0" maxOccurs="1" />
      <xs:element name="Name" minOccurs="1" maxOccurs="1" />
      <xs:element name="SystemName" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="InitializationDataFile" type="InitializationDataFileType" />
  <xs:complexType name="ScenarioSettingType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ScenarioSetting</xs:documentation>
      <xs:documentation>"Information describing the setting of the scenario, including the date, time, physical extent of the terrain, and a version string."</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Version" minOccurs="1" maxOccurs="1" />
      <xs:element name="DateTime" minOccurs="1" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ScenarioSetting" type="ScenarioSettingType" />
  <xs:complexType name="ObjectDefinitionsType">
    <xs:annotation>
      <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ObjectDefinitions</xs:documentation>
      <xs:documentation>"Information characterizing an object"</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PlanPhaseReference" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="Action" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="Entity" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="AbstractObject" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ObjectDefinitions" type="ObjectDefinitionsType" />
</xs:schema>